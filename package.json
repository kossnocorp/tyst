{
  "name": "tysts",
  "version": "0.0.0",
  "description": "Test framework for TypeScript types",
  "author": "Sasha Koss <koss@nocorp.me>",
  "license": "MIT",
  "packageManager": "pnpm@10.18.3+sha512.bbd16e6d7286fd7e01f6b3c0b3c932cda2965c06a908328f74663f10a9aea51f1129eea615134bf992831b009eabe167ecb7008b597f40ff9bc75946aadfb08d",
  "files": [
    "*.md",
    "lib"
  ],
  "type": "module",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "import": "./src/index.ts"
    }
  },
  "publishConfig": {
    "main": "./lib/index.js",
    "types": "./lib/index.d.ts",
    "exports": {
      "./package.json": "./package.json",
      ".": {
        "require": {
          "types": "./index.d.cts",
          "default": "./index.cjs"
        },
        "import": {
          "types": "./index.d.ts",
          "default": "./index.js"
        }
      }
    }
  },
  "dependencies": {
    "tdollar": "workspace:^"
  },
  "devDependencies": {
    "typescript": "5.9.3"
  }
}
